{
  "name": "reactnativerxjs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "ios": "npx pod-install && npx react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "android": "npx react-native run-android --variant=Debug",
    "backend": "json-server --watch db.json --port 5000 --delay=1000",
    "start:fullstack": "concurrently \"npm run backend\" \"npm run ios\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-elements": "^3.4.2",
    "react-native-safe-area-context": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "concurrently": "^6.2.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "17.0.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
